{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["node"]
  },
  "baseUrl": "./",
  "paths": {
    "@shared/*": ["src/shared/*"],
    "@widgets/*": ["src/widgets/*"],
    "@entities/*": ["src/entities/*"],
    "@features/*": ["src/features/*"],
    "@pages/*": ["src/pages/*"],
    "@assets/*": ["src/assets/*"],
    "@redux/*": ["src/redux/*"]
  }
}
