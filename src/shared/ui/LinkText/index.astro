  ---
  import React from "react";
  import { Link } from "react-scroll";

  import styles from "../LinkText/styles.module.scss";

  interface Props {
    text: string;
    to: string;
  }

  const { text, to } = Astro.props;

  ---

  <Link to={to} className={styles.linktext} smooth>
    {text}
  </Link>
